<hibernate-mapping package="java">
	<class name="Conductor" table="CONDUCTOR">
		<id name="IdUsuario" column="ID_USUARIO">
			  <generator class="native"/>
		</id>
		
		<set name="viajes" cascade="all">
			<key column="ID_USUARIO"
			not-null="true"/>
			<one-to-many class="Viaje"/>
		</set>
	</class>
</hibernate-mapping>